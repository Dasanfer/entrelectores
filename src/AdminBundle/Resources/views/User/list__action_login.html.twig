{% if admin.isGranted('EDIT', object) and admin.hasRoute('login') %}
    <a href="{{ admin.generateObjectUrl('login', object) }}" class="btn btn-sm btn-default " title="{{ 'action_login'|trans({}, 'SonataAdminBundle') }}">
        <i class="glyphicon glyphicon-user"></i>
        Impersonar
    </a>
{% endif %}