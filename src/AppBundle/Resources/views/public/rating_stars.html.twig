{%if star_count is not defined%}
<ul data-rating="{{rating}}" class="rating {{class}}" data-id={{id}} data-type="{{type}}">
    <li class="empty">
        <span class="l" data-value="1"></span>
        <span class="r" data-value="2"></span>
    </li>
    <li class="empty">
        <span class="l" data-value="3"></span>
        <span class="r" data-value="4"></span>
    </li>
    <li class="empty">
        <span class="l" data-value="5"></span>
        <span class="r" data-value="6"></span>
    </li>
    <li class="empty">
        <span class="l" data-value="7"></span>
        <span class="r" data-value="8"></span>
    </li>
    <li class="empty">
        <span class="l" data-value="9"></span>
        <span class="r" data-value="10"></span>
    </li>
</ul>
{%else%}
<ul class="rating">
    {%for i in 1..star_count%}
    <li class="empty">
    <span class="l"></span>
    <span class="r"></span>
    </li>
    {%endfor%}
</ul>
{%endif%}
